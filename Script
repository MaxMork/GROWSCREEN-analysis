  countsdf$ShootIDs <- unique(counttemp$ShootIDs)
  countsdf$Genotype <- unique(counttemp$Genotype)
  countsdf$stress <- unique(counttemp$stress)
  countsdf$gene <- unique(counttemp$gene)
  countsdf$GT <- unique(counttemp$GT)
