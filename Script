  scale_shape_manual(values=c(19,19,19,19,19,19,19,19,19,19,19,19,19,19))+
  scale_size_manual(values=c(2,2,2,2,2,2,2,2,2,2,2,2,2,2,2,2,2,2,2,2,2,2,2,2,2,2,2,2,2))+
