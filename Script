  scale_size_manual(values=c(2,2,2,2,2,2,2,2,2,2,2,2,2,2))+
  theme_bw() + theme(legend.title = element_blank(),text = element_text(size = 18))+
  xlab("DAS") + ylab("plantSurfaceCoverage") +labs(title = "plantSurfaceCoverage")
