    xlab("Fv_Fm") + ylab(trait) +labs(title = titlename)
  ggsavename <- paste(trait, "_vs_Fv_Fm_OE_GT.png", sep = "")
  ggsave(ggsavename, plot = last_plot(),width = 7, height =5)
  
  
